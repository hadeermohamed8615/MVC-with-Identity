@model Employee




<div class="card" style="width: 18rem;">
    <img class="card-img-top" src="/Images/@Model.Image" alt="Card image cap">
    <div class="card-body">
        <h5 class="card-title">@Model.Id</h5>
        <p class="card-text">@Model.Name"</p>
        <a href="/Employee/Index" class="btn btn-primary">Go To List</a>
    </div>
</div>
